{
  "manifest_version": 3,
  "name": "Censor.net RSS Agent",
  "version": "1.0",
  "description": "Stay informed with the latest Ukrainian news directly from Censor.net with Censor.net RSS Agent Chrome extension",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon_16.png",
      "48": "icon_48.png",
	  "64": "icon_64.png",
      "128": "icon_128.png"
    }
  },
  "permissions": [
    "https://censor.net/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": [
    "https://censor.net/*"
  ]
}