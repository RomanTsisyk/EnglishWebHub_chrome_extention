{
  "manifest_version": 2,
  "name": "Productivity Booster",
  "version": "1.0",
  "description": "A productivity extension implementing the Pomodoro technique",
  "permissions": ["alarms", "notifications"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "browser_action": {
  "default_popup": "popup.html"
}
}
